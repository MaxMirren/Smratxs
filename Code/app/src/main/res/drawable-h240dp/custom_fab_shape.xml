<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
            android:gravity="center"
            android:top="@dimen/fab_width_height"
            android:bottom="@dimen/fab_width_height"
            android:left="@dimen/fab_width_height"
            android:right="@dimen/fab_width_height"
    >
        <rotate android:toDegrees="45"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:fromDegrees="45">
            <shape android:shape="rectangle">
                <corners android:radius="5dp"/>
                <solid android:color="@color/colorAccent"/>
                <size android:height="37dp" android:width="37dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>